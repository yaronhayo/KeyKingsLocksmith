{
  "buildCommand": "npm run build",
  "framework": "astro",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-DNS-Prefetch-Control",
          "value": "on"
        },
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(self), interest-cohort=()"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://www.google.com https://www.gstatic.com https://maps.googleapis.com https://www.googletagmanager.com https://www.google-analytics.com https://www.clarity.ms https://scripts.clarity.ms https://cdn.vercel-insights.com https://va.vercel-scripts.com; style-src 'self' 'unsafe-inline' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https: blob:; connect-src 'self' https://www.google.com https://maps.googleapis.com https://www.google-analytics.com https://analytics.google.com https://api.resend.com https://www.clarity.ms https://*.clarity.ms https://www.gstatic.com https://vitals.vercel-insights.com; frame-src https://www.google.com; media-src 'self' https://*.blob.vercel-storage.com blob:; object-src 'none'; base-uri 'self'; form-action 'self'; frame-ancestors 'self';"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*\\.(?:jpg|jpeg|png|gif|webp|svg|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(?:css|js))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/services.html",
      "destination": "/services",
      "permanent": true
    },
    {
      "source": "/contact.html",
      "destination": "/contact",
      "permanent": true
    },
    {
      "source": "/about.html",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/locksmith-anderson-sc",
      "destination": "/service-areas/anderson-sc",
      "permanent": true
    },
    {
      "source": "/locksmith-greenville-sc",
      "destination": "/service-areas/greenville-sc",
      "permanent": true
    },
    {
      "source": "/locksmith-clemson-sc",
      "destination": "/service-areas/clemson-sc",
      "permanent": true
    },
    {
      "source": "/emergency",
      "destination": "/services/emergency-locksmith",
      "permanent": true
    },
    {
      "source": "/car-lockout",
      "destination": "/services/automotive-locksmith",
      "permanent": true
    },
    {
      "source": "/house-lockout",
      "destination": "/services/residential-locksmith",
      "permanent": true
    },
    {
      "source": "/commercial",
      "destination": "/services/commercial-locksmith",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap-index.xml"
    },
    {
      "source": "/security.txt",
      "destination": "/.well-known/security.txt"
    }
  ],
  "crons": []
}
